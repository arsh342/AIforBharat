{
  "name": "voice-civic-assistant",
  "version": "1.0.0",
  "description": "Voice-First Civic Assistant for PM-JAY eligibility and health grievances",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:property": "python3 -m pytest test/property/ -v",
    "test:all": "npm run test && npm run test:property",
    "cdk": "cdk",
    "deploy": "cdk deploy",
    "destroy": "cdk destroy",
    "synth": "cdk synth",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "setup:python": "pip3 install -r requirements.txt",
    "postinstall": "npm run setup:python"
  },
  "keywords": [
    "voice-assistant",
    "civic-tech",
    "aws",
    "serverless",
    "ai",
    "healthcare"
  ],
  "author": "Voice Civic Assistant Team",
  "license": "MIT",
  "devDependencies": {
    "@types/aws-lambda": "^8.10.130",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "aws-cdk": "^2.108.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@aws-sdk/client-bedrock-runtime": "^3.454.0",
    "@aws-sdk/client-dynamodb": "^3.454.0",
    "@aws-sdk/client-rekognition": "^3.454.0",
    "@aws-sdk/client-s3": "^3.454.0",
    "@aws-sdk/client-transcribe": "^3.454.0",
    "@aws-sdk/lib-dynamodb": "^3.454.0",
    "aws-cdk-lib": "^2.108.0",
    "aws-lambda": "^1.0.7",
    "constructs": "^10.3.0",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.130",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "aws-cdk": "^2.108.0",
    "eslint": "^8.54.0",
    "fast-check": "^3.15.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
